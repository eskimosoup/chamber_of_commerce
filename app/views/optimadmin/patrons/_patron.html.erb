<div class="row collapse module-list-index <%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%= patron_presenter.id %>">
  <div class="large-7 small-4 columns" data-equalizer-watch="module-list">
    <%= patron_presenter.name %>
  </div>

  <div class="large-2 hide-for-medium-down columns" data-equalizer-watch="module-list">
    <%= patron_presenter.edit_image(:image) %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= patron_presenter.toggle_link %>
  </div>

  <div class="large-1 small-4 text-center columns" data-equalizer-watch="module-list">
    <%= patron_presenter.edit_link %>
  </div>

  <div class="large-1 hide-for-medium-down text-center columns last" data-equalizer-watch="module-list">
    <%= patron_presenter.delete_link %>
  </div>
</div>
