<div class="row collapse module-list-index <%= cycle('odd', 'even') %>" data-equalizer="module-list" id="item_<%= member_presenter.id %>">
  <div class="large-1 columns" data-equalizer-watch="module-list">
    <%= member_presenter.chamber_db_id %>
  </div>
  <div class="large-4 small-6 columns" data-equalizer-watch="module-list">
    <%= member_presenter.title %>
  </div>

  <div class="large-3 hide-for-small columns" data-equalizer-watch="module-list">
    <% member_presenter.member_logins.each do |member_login| %>
      <%= link_to "Edit #{member_login.username} (#{member_login.contact_name})", edit_member_member_login_path(member_login.member, member_login) %><br />
    <% end %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= member_presenter.toggle_link(:verified) %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= member_presenter.show_link %>
  </div>

  <div class="large-1 small-4 columns" data-equalizer-watch="module-list">
    <%= member_presenter.edit_link %>
  </div>

  <div class="large-1 hide-for-medium-down columns last" data-equalizer-watch="module-list">
    <%= member_presenter.delete_link %>
  </div>

  <div class="row module-list-detail hide" id="index-list-<%= member_presenter.id %>">
    <div class="small-12 columns module-list-summary last">

    </div>
  </div>
</div>
