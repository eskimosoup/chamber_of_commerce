<section class="content-area">
  <div class="row">
    <div class="large-8 small-12 columns">
      <div class="main-content-wrap">
        <h1><%= @page.title %></h1>

        <div class="content">
          <%= @page.image %>

          <%= @page.content %>

          <ul>
            <%= nested_menu_items(menu_items: @members_services_menu.menu_items) %>
          </ul>
        </div>
      </div>
    </div>

    <div class="large-4 small-12 columns">
      <%= render 'shared/sidebar' %>
    </div>
  </div>
</section>
