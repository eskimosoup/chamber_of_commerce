<div class="row">
  <div class="small-12 section large columns" data-equalizer="row-content-box-1">
    <h2 class="full-header"><div>Chamber Group</div></h2>

    <div class="row">
      <div class="small-4 columns">
        <div class="content-box"  data-equalizer-watch="row-content-box-1">
          <div class="content-box-area-title">
            <%= two_tone_title "International Trade" %>
          </div>

          <div class="inner text-center">
            <ul class="plain-list">
              <li>
                <%= link_to 'Google', '#' %>
              </li>
            </ul>
          </div>

          <div class="text-center">
            <%= link_to 'More on International Trade', '#', class: 'content-box-ghost-button' %>
          </div>

          <div class="button-text-center">
            <%= image_tag 'components/international-trade/good-travel-logo.jpg', class: 'Good Travel Logo' %>
            <%= image_tag 'components/international-trade/aa-logo.jpg', class: 'AA Global' %>
          </div>
        </div>
      </div>

      <div class="small-4 columns">
        <div class="content-box" data-equalizer-watch="row-content-box-1">
          <div class="content-box-area-title">
            <%= two_tone_title "Policy and Representation" %>
          </div>

          <div class="inner text-center">
            <ul class="plain-list">
              <li>
                <%= link_to 'Google', '#' %>
              </li>
            </ul>
          </div>

          <div class="button-text-center">
            <%= link_to 'More on Policy and Representation', '#', class: 'content-box-ghost-button small' %>
          </div>
        </div>
      </div>

      <div class="small-4 columns">
        <div class="chamber-information"  data-equalizer-watch="row-content-box-1">
          <div class="preview-first-home-item">
            <div class="row collapse">
              <div class="small-6 columns">
                <%= image_tag 'components/training/photo.jpg' %>
              </div>

              <div class="small-6 columns">
                <div class="content">
                  <div class="title">
                    <%= two_tone_title "Chamber Training" %>
                  </div>

                  <p class="summary">
                    Your new career starts here
                  </p>
                </div>
              </div>
            </div>
          </div>

          <div class="preview-second-home-item">
            <div class="row collapse">
              <div class="small-6 columns">
                <%= image_tag 'components/acorn/logo.jpg', alt: 'Chamber Acorn Fund' %>
              </div>

              <div class="small-6 columns">
                <div class="content">
                  <div class="title">
                    Chamber <span class="secondary">Acorn Fund</span>
                  </div>

                  <p class="summary">
                    Access to business and finance help
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
